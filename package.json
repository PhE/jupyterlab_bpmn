{
  "author": "Philippe ENTZMANN",
  "description": "A JupyterLab extension for viewing BPMN files.",
  "dependencies": {
    "@jupyterlab/application": "^0.9.0",
    "@jupyterlab/rendermime-interfaces": "^0.3.0",
    "@phosphor/widgets": "^1.5.0",
    "bpmn-js": "^0.21.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/*.{gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "jupyterlab_bpmn",
  "repository": {
    "type": "git",
    "url": "https://github.com/PhE/jupyterlab_bpmn.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build"
  },
  "version": "0.1.5"
}
